page {
  background-color: #eee;
}
.franchisee-detail-wrap .slick-carousel-container {
  width: 100%;
  height: 20rem;
  border-bottom:1px solid #eee;
  background:#fff;
  text-align: center;
  overflow: hidden;
}
swiper {
  height: 20rem;
}
.franchisee-detail-wrap .franchisee-cover-container {
  background-color: #fff;
  height: 20rem;
  width: 100%;
}
.franchisee-detail-wrap .slick-carousel-container image,
.franchisee-detail-wrap .franchisee-cover-container image {
  width: 100%;
  height: 100%;
}
.franchisee-detail-wrap .slick-slide:after {
  content: '';
  display: inline-block;
  height: 100%;
  vertical-align: middle;
}
.franchisee-detail-wrap .slick-slide {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
          align-items: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.franchisee-detail-wrap .slick-slide image {
  width: 100%;
  height: auto;

}
.franchisee-section{
    background: rgba(0,0,0,.3);
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 20rem;
}
.franchisee-detail-wrap .franchisee-detail-section {
    z-index: 20;
    background: #fff;
   
}

.franchisee-detail-wrap .franchisee-title {
  padding: 10px 10px;
  font-size:36rpx;
  color: #323232;
}
.franchisee-detail-wrap .franchisee-other-detail {
  padding: 0 0 0 10px;
  font-size: 26rpx;
  color: #323232;
  margin-bottom: 10px;
}
.franchisee-address {
  display: inline-block;
  width: calc(100%);
  vertical-align: top;
  word-break: break-all;
  vertical-align: baseline;
  white-space: nowrap;
  text-overflow:ellipsis;
  text-overflow: ellipsis;
  overflow:hidden;
  padding: 10px 0;
  border-top: 1px solid #eee;
}
.franchisee-phone {
  padding: 10px 0;
  border-top: 1px solid #eee;
}
.franchisee-detail-wrap .right-arrow-bar {
  padding-right: 33px;
}
.franchisee-detail-wrap .right-arrow-bar:before {
  font-size: 16px;
}

.franchisee-goods-list-container {
  background-color: #fff;
  margin-bottom:10px;
}
.franchisee-goods-list-title {
  padding: 10px;
  border-bottom: 1px solid #f8f8f8;
  font-size: 28rpx;
  color: #323232;
}
.franchisee-goods-empty-tip {
  padding: 10px;
  color: #999;
  text-align: center;
}
.franchisee-goods-item {
  position: relative;
  height: 60px;
  padding: 10px;
  margin-bottom: 1px;
  text-decoration: inherit;
  border-bottom: 1px solid #f8f8f8;
}
.franchisee-goods-item .list-img {
  display: inline-block;
  width: 80px;
  height: 60px;
  vertical-align: middle;
}
.franchisee-goods-item .title-container {
  display: inline-block;
  width: calc(100% - 80px);
  text-decoration: inherit;
  vertical-align: top;
}
.franchisee-goods-item .title-container view {
  width: calc(100% - 10px);
  padding-left: 10px;
  word-break: break-all;
}
.franchisee-goods-item .title-container view:last-child {
  position: absolute;
  bottom: 10px;
}
.franchisee-goods-item .title-container .title {
  text-decoration: inherit;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  font-size: 28rpx;
}
.franchisee-goods-item .title-container .price {
  color: #ff7100;
  margin-right: 10px;
  font-size: 32rpx;
}
.franchisee-goods-item .title-container .sales {
  font-size: 26rpx;
  color: #a8a8a8;
}
.franchisee-tip-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 50px;
  line-height: 50px;
  font-size: 16px;
  background-color: rgba(0,0,0,.6);
  color: #fff;
  text-align: center;
}
.franchisee-goods-tab {
  display: inline-block;
}
.franchisee-goods-tab text {
  display: block;
  padding: 23.4rpx 70.2rpx;
  color: #666;
  border-bottom: 1px solid transparent;
}
.franchisee-goods-tab .selected {
  color: #00f010;
  border-bottom: 1px solid #00f010;
}
.franchisee-goods-tab-container {
  text-align: center;
}
.franchisee-other-detail .ico-moon {
  font-size: 26rpx;
  color: #bababa;
  vertical-align: text-top;
}
.franchisee-goods-coupon-container{
    position: relative;
    background-color: #fff;
    height: 80px;
    width: 100%;
    overflow-x: scroll;
    white-space: nowrap;
}
.franchisee-goods-coupon-container .scroll-view-item{
  margin: 10px 0 10px 10px;
    width: 201px;
    background: #fee1db;
    height: 60px;
    display: inline-block;
    white-space: normal;
    border-radius: 2px;
    position: relative;
}
.coupon-left,.coupon-right{
  float: left;
  cursor: pointer;
}
.coupon-left{
    border-right: 1px dotted #ed6d64;
}
.coupon-left .coupon-price{
  float: left;
    text-align: center;
    height: 100%;
    line-height: 60px;
    width: 70px;
    font-size: 18px;
    color: #ff5032;
}
.coupon-left .coupon-news{
   float: left;
    width: 100px;
    height: 60px;
    color: #ed6d64;
}
.coupon-news .con{
  margin-top: 5px;
  font-size: 14px;
  padding:3px 0;
}
.coupon-right{
  margin: 0 auto;
    font-size: 16px;
    width: 30px;
    height: 100%;
    text-align: center;
    color: #ed6d64;
    line-height: 30px;
}
.scroll-view-item::before{
   content: '';
    width: 10px;
    height: 10px;
    border-radius: 50%;
    background: #fff;
    z-index: 999;
    display: block;
    position: absolute;
    left: 166px;
    top: -5px;
}
.scroll-view-item::after{
  content: '';
    width: 10px;
    height: 10px;
    border-radius: 50%;
    background: #fff;
    z-index: 999;
    display: block;
    position: absolute;
    left: 166px;
    top: 55px;
}
.get-goods-coupon{
    padding: 10px;
    margin-top: 10px;
    margin-bottom: 10px;
    background: #fff; 
    font-size:26rpx;
    color: #323232;
}
.franchisee-goods-content{
  width: 100%;
  background: #fff;
  padding-left: 10px;
  margin-top:10px;
}
.franchisee-con{
  height: 40px;
  line-height: 40px;
  font-size: 28rpx;
  border-bottom: 1px solid #e1e1e1;
}
.franchisee-goods-content .content-news{
    height:auto;
    font-size: 26rpx;
    border-bottom: 1px solid #e1e1e1;
    padding: 10px 20px 10px 0;
}
.content-time{
  padding: 10px 0;
  font-size: 26rpx;
  color: #666;
}
.content-time label{
  display: inline-block;
  line-height: 1.5em;
  vertical-align: top;

}
.content-time .franchisee-business-hours{
    line-height: 1.5em;
    display: inline-block;
    width: 230px;
    vertical-align: top;
    color: #323232;
}
.franchisee-goods-content .content-news .con_des{
    width: 100%;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
    overflow: hidden;
    line-height: 22px;
    color: #666666;
}
.content-news .franchisee-description{
    width:100%;
    line-height: 22px;
    color:#323232;
}